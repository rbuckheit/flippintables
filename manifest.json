{
  "name": "AngelHack",
  "version": "1.0",
  "description": "Stores visual representation of user's navigation between pages",
  "browser_action": {
    "default_icon": "icon.png",
	"default_popup" : "popup.html"
  },
  "content_scripts": [ {
    "matches": ["<all_urls>"],
    "js": ["jquery-1.7.min.js", "utils.js", "link_listener.js", "load_listener.js"]
  } ],
  "permissions": [
    "http://api.flickr.com/"
  ],
  "background_page": "background.html"
}